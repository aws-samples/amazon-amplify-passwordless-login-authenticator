{
  "function": {
    "passwordlessCreateAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "passwordlessDefineAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "passwordlessVerifyAuthChallengeResponse": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "auth": {
    "passwordless": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "passwordlessCreateAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "passwordlessDefineAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "passwordlessVerifyAuthChallengeResponse",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": true,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  }
}